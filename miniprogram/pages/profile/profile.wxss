page {
  background-color: #f1f1f1;
}

.profile-header {
  background-image: url(data:image/jpeg;base64,);
  background-size: cover;
  height: 480rpx;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 1)
}

.avatar-url {
  width: 200rpx;
  height: 200rpx;
  display: block;
  overflow: hidden;
  border: 6rpx solid #fff;
  border-radius: 50%;
  margin-bottom: 10rpx;
}

.nickname {
  font-weight: 700;
}

.nav {
  margin-top: 40rpx;
}

.nav-item {
  display: flex;
  position: relative;
  align-items: center;
  width: 90%;
  margin: 0 auto;
  background-color: white;
  border: 1px solid #eee;
  padding: 10rpx 30rpx;
}

.content {
  width: 100%;
  height: 100%;
}

.nav-item:nth-of-type(2),.nav-item:nth-of-type(3) {
  border-top: 0px;
}

.nav-item:first-of-type {
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
}

.nav-item:last-of-type {
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
}

.icon-playing,.icon-save,.icon-qrcode {
  font-size: 32rpx;
  font-weight: 700;
  color: #c43d43;
  margin-right: 16rpx;
}

.icon-right {
  position: absolute;
  right: 30rpx;
}